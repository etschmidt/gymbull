<% provide(:title, @title) %>

<div class="row">
	 
	<div class="col-md-1">
    <%= render 'layouts/header' %>
  </div>
  
  <div class="col-md-11">
  	
	  <h1>Recent Activity</h1>
	
		<% @activities.each do |activity| %>
			<div class="activities">
				<%= render_activity activity %>
			</div>
		<% end %>
		<%= will_paginate @activities %>
	
	</div>
</div>